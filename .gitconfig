[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  rbc = rebase --continue
  ap = add -p
  rbi = rebase -i
	up = pull --rebase --autostash
